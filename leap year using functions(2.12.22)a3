def is_leap(year):
    if year % 400 == 0:
        return True
    elif year % 100 == 0:
        return False
    elif year % 4 == 0:
        return True
year = int(input("year="))
if(is_leap(year)):
    print("Leap year")
else:
    print("Not a Leap year")


def is_leap(year):
    if year % 400 == 0:
        print("Leap year")
    elif year % 100 == 0:
         print("Not Leap year")
    elif year % 4 == 0:
         print("Leap year")
year = int(input("year="))
is_leap(year)

 
def is_leap(year):
    if year % 400 == 0:
        return "Leap year"
    elif year % 100 == 0:
        return "Not Leap year"
    elif year % 4 == 0:
        return "Leap year"
year = int(input("year=")) 
print(is_leap(year))
